        tr
          td='Lampu ' + lamp[7].dataValues.lamp_id
          td(id='lamp_8')=lamp[7].dataValues.lamp_current + ' Amp'
        tr
          td='Lampu ' + lamp[8].dataValues.lamp_id
          td(id='lamp_9')=lamp[8].dataValues.lamp_current + ' Amp'

  script(src="/javascripts/lamp_fe.js")